<ul class="students">
  <% for(var i=0; i<students.length; i++) {%>
    <li class="student">
      <span><%= students[i].githubName %>
      </span><br />
      <a href="/students/<%= students[i].githubId %>">View</a>
      <a href="/students/<%= students[i].githubId %>/edit">Edit</a>
    </li>
  <% } %>
</ul>
